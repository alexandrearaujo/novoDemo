{
	"name": "modulo",
	"description": "bower components for modulo UI.",
    "version": "1.0.0",
    "dependencies": {
    "bootstrap": "latest",
    "bootstrap-confirmation2": "latest",
    "bootstrap-datepicker": "latest",
    "bootstrap-table": "latest",
    "bootstrap-timepicker": "latest",
    "bootstrapvalidator": "latest",
    "font-awesome": "latest",
    "iCheck": "latest",
    "jquery": "latest",
    "jQuery-Mask-Plugin": "latest",
    "knockout": "latest",
    "metisMenu": "latest",
    "remarkable-bootstrap-notify": "latest",
    "screenfull": "latest",
    "typeahead.js": "0.10.5"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap-theme.min.css",
        "dist/css/bootstrap-theme.css.map",
        "dist/js/bootstrap.min.js",
        "dist/fonts/glyphicons-halflings-regular.eot",
    	"dist/fonts/glyphicons-halflings-regular.svg",
    	"dist/fonts/glyphicons-halflings-regular.ttf",
    	"dist/fonts/glyphicons-halflings-regular.woff",
    	"dist/fonts/glyphicons-halflings-regular.woff2"
      ]
    },
    "bootstrap-confirmation2": {
      "main": [
        "bootstrap-confirmation.min.js"
      ]
    },
    "bootstrap-datepicker": {
      "main": [
        "dist/css/bootstrap-datepicker3.min.css",
        "dist/js/bootstrap-datepicker.min.js",
        "dist/locales/bootstrap-datepicker.pt-BR.min.js"
      ]
    },
    "bootstrap-table": {
      "main": [
        "dist/bootstrap-table.min.css",
        "dist/bootstrap-table.min.js",
        "dist/locale/bootstrap-table-pt-BR.min.js"
      ]
    },
    "bootstrap-timepicker": {
      "main": [
        "css/bootstrap-timepicker.min.css",
        "js/bootstrap-timepicker.min.js"
      ]
    },
    "bootstrapvalidator": {
      "main": [
        "dist/css/bootstrapValidator.min.css",
        "dist/js/bootstrapValidator.min.js",
        "dist/js/language/pt_BR.js"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "css/font-awesome.css.map",
        "fonts/**"
      ]
    },
    "iCheck": {
      "main": [
        "icheck.min.js",
        "skins/minimal/blue.css",
        "skins/minimal/blue.png"
      ]
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    "jQuery-Mask-Plugin": {
      "main": [
        "dist/jquery.mask.min.js"
      ]
    },
    "knockout": {
      "main": [
        "dist/knockout.js"
      ]
    },
    "metisMenu": {
      "main": [
        "dist/metisMenu.min.css",
        "dist/metisMenu.min.js"
      ]
    },
    "remarkable-bootstrap-notify": {
      "main": [
        "dist/bootstrap-notify.min.js"
      ]
    },
    "screenfull": {
      "main": [
        "dist/screenfull.js"
      ]
    },
    "typeahead.js": {
      "main": [
        "dist/typeahead.bundle.min.js"
      ]
    }
  },
  "devDependencies": {},
  "resolutions": {
	"jquery": "2.1.4",
    "bootstrap": "3.3.4"
  }
}